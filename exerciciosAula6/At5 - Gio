#include <stdio.h>
#include <math.h>

int main() {
    double cat1, cat2, hip;

    printf("Digite o valor do primeiro cateto: ");
    scanf("%lf", &cat1);

    printf("Digite o valor do segundo cateto: ");
    scanf("%lf", &cat2);

    hip = sqrt(pow(cat1, 2) + pow(cat2, 2));

    printf("O valor da hipotenusa Ã©: %.2lf\n", hip);

    return 0;
